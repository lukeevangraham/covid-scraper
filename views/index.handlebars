<div class="container">


<table class="table">
    <thead>
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Total Positives</th>
            <th scope="col">In Hospital</th>
            <th scope="col">In ICU </th>
            <th scope="col">Deaths</th>
        </tr>
    </thead>
    <tbody>
    {{#each stats}}
    {{> stat-block }}
    {{/each }}

    </tbody>
</table>

<p class="blockquote-footer text-center font-italic"><em>All data taken from <cite title="Source Title">sandiegocounty.gov</cite></em></p>




</div>